            <li class="<%- title=='home'?'hidden-sm hidden-md active':'hidden-sm hidden-md' %>" ><a href="/" >首页</a></li>
            <li data-code="4000"  class="<%- title=='login'?'active':'' %>" > <a class="reddot" href="/login" >登录</a></li>
