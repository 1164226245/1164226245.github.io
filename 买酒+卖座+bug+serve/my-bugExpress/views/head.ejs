    <script>
        //前端发起ajax请求拿到session
         $.ajax({
            url:"/checksession",
            type:"GET",
            // dataType:"json",
            // data:{
            //     id:1910
            // },
            // headers:{
                
            // },
            success(res){
                // console.log(res);
                var lis = $("#ullist").find("li");
                lis.each((index,item)=>{
                    if(index!==0){
                        if($(item).attr("data-code")==res.code){
                            $(item).show();
                        }else{
                            $(item).hide();
                        }
                    }
                })
            }
        })


    </script>